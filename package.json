{
  "name": "guanshu-component-library",
  "version": "1.0.0",
  "description": "观数浏览器扩展组件库",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.showcase.config.ts",
    "build": "vite build --config vite.showcase.config.ts",
    "build:lib": "vite build --config vite.lib.config.ts && tsc -p tsconfig.build.json"
  },
  "keywords": [
    "component",
    "library",
    "react",
    "antd"
  ],
  "authors": [
    "观数团队"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "antd": "^5.27.1",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.2.1",
    "less": "^4.4.1",
    "typescript": "^5.8.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "antd": ">=5.0.0",
    "@ant-design/icons": ">=5.0.0"
  }
}
